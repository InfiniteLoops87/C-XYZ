<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/rootbg"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginLeft="3dp"
		android:layout_marginTop="15dp"
		android:layout_marginRight="3dp"
		android:layout_marginBottom="3dp"
		android:background="#181818"
		android:orientation="vertical"
		android:animateLayoutChanges="true">
		<LinearLayout
			android:id="@+id/currentFocusedView"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/op1"
				android:layout_width="60dp"
				android:layout_height="match_parent"
				android:gravity="center_horizontal|bottom"
				android:orientation="vertical">
				<ImageView
					android:id="@+id/ic1"
					android:focusable="false"
					android:layout_width="35dp"
					android:layout_height="35dp"
					android:padding="8dp"
					android:src="@drawable/content_device"
					android:scaleType="fitCenter" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/LinearLayout4"
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:orientation="vertical" />
			<LinearLayout
				android:id="@+id/vline"
				android:layout_width="1dp"
				android:layout_height="match_parent"
				android:layout_marginTop="5dp"
				android:layout_marginBottom="5dp"
				android:padding="8dp"
				android:background="#474747"
				android:orientation="vertical" />
			<LinearLayout
				android:id="@+id/op2"
				android:layout_width="40dp"
				android:layout_height="match_parent"
				android:gravity="center_horizontal|bottom"
				android:orientation="vertical">
				<ImageView
					android:id="@+id/ic2"
					android:focusable="false"
					android:layout_width="35dp"
					android:layout_height="35dp"
					android:padding="8dp"
					android:src="@drawable/content_github"
					android:scaleType="fitCenter" />
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/main_fore"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear2"
				android:layout_width="match_parent"
				android:layout_height="2dp"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/line1"
					android:layout_width="60dp"
					android:layout_height="2dp"
					android:padding="8dp"
					android:background="#FF0000"
					android:orientation="horizontal" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/LinearLayout3"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal"
				android:animateLayoutChanges="true">
				<ListView
					android:id="@+id/listview1"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:choiceMode="none"
					android:fillViewport="true"
					android:fadingEdgeLength="20dp"
					android:requiresFadingEdge="vertical"
					android:fadeScrollbars="false"
					android:overScrollMode="never"
					android:scrollbars="none" />
				<LinearLayout
					android:id="@+id/sec"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="vertical">
					<LinearLayout
						android:id="@+id/LinearLayout6"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/clone"
							android:layout_width="wrap_content"
							android:layout_height="35dp"
							android:layout_marginLeft="6dp"
							android:layout_marginTop="6dp"
							android:layout_marginRight="3dp"
							android:layout_marginBottom="6dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal"
							android:layout_weight="1">
							<LinearLayout
								android:id="@+id/LinearLayout5"
								android:layout_width="40dp"
								android:layout_height="match_parent"
								android:padding="8dp"
								android:orientation="vertical" />
							<ImageView
								android:id="@+id/up"
								android:focusable="false"
								android:layout_width="40dp"
								android:layout_height="35dp"
								android:padding="8dp"
								android:src="@drawable/content_add"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview4"
								android:focusable="false"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_marginRight="50dp"
								android:padding="8dp"
								android:text="Clone git"
								android:textSize="14sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/gotopath"
							android:layout_width="wrap_content"
							android:layout_height="35dp"
							android:layout_marginLeft="3dp"
							android:layout_marginTop="6dp"
							android:layout_marginRight="6dp"
							android:layout_marginBottom="6dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/imageview1"
								android:focusable="false"
								android:layout_width="40dp"
								android:layout_height="35dp"
								android:padding="8dp"
								android:src="@drawable/content_folder"
								android:scaleType="fitCenter" />
						</LinearLayout>
					</LinearLayout>
					<ListView
						android:id="@+id/listview2"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:choiceMode="none"
						android:fillViewport="true"
						android:fadingEdgeLength="20dp"
						android:requiresFadingEdge="vertical"
						android:fadeScrollbars="false"
						android:overScrollMode="never"
						android:scrollbars="none" />
					<LinearLayout
						android:id="@+id/layout_no_items"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:visibility="gone">
						<ImageView
							android:id="@+id/no_file_folder"
							android:focusable="false"
							android:layout_width="40dp"
							android:layout_height="25dp"
							android:layout_marginTop="10dp"
							android:layout_marginBottom="3dp"
							android:src="@drawable/inbox_empty"
							android:scaleType="fitCenter" />
						<TextView
							android:id="@+id/textview3"
							android:focusable="false"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="16dp"
							android:paddingTop="3dp"
							android:paddingRight="16dp"
							android:paddingBottom="8dp"
							android:gravity="center_horizontal|center_vertical"
							android:text="No cloned git."
							android:textSize="12sp"
							android:textColor="#FFFFFF" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
