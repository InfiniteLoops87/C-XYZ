<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/background1"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:gravity="center_vertical"
		android:orientation="horizontal"
		android:animateLayoutChanges="true">
		<com.inflps.codexyz.SwipeRevealLayout
			android:id="@+id/swipe_reveal1"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			app:mode="same_level"
			app:dragEdge="left"
			android:animateLayoutChanges="true">
			<LinearLayout
				android:id="@+id/archive"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_marginTop="4dp"
				android:layout_marginBottom="4dp"
				android:background="@android:color/transparent"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal"
				android:animateLayoutChanges="true">
				<LinearLayout
					android:id="@+id/LinearLayout12"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/more"
						android:layout_width="40dp"
						android:layout_height="match_parent"
						android:layout_margin="3dp"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:visibility="gone">
						<ImageView
							android:id="@+id/imageview10"
							android:focusable="false"
							android:layout_width="35dp"
							android:layout_height="25dp"
							android:padding="3dp"
							android:src="@drawable/leftar"
							android:scaleType="fitCenter" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/l15"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:orientation="horizontal">
						<LinearLayout
							android:id="@+id/moreMode"
							android:layout_width="40dp"
							android:layout_height="match_parent"
							android:layout_margin="3dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview5"
								android:focusable="false"
								android:layout_width="30dp"
								android:layout_height="30dp"
								android:padding="3dp"
								android:src="@drawable/content_menu_vertical"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview9"
								android:focusable="false"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="More"
								android:textSize="11sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/copyMode"
							android:layout_width="40dp"
							android:layout_height="match_parent"
							android:layout_margin="3dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview6"
								android:focusable="false"
								android:layout_width="30dp"
								android:layout_height="30dp"
								android:padding="3dp"
								android:src="@drawable/content_copy"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview10"
								android:focusable="false"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Copy"
								android:textSize="11sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/cutMode"
							android:layout_width="40dp"
							android:layout_height="match_parent"
							android:layout_margin="3dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview7"
								android:focusable="false"
								android:layout_width="30dp"
								android:layout_height="30dp"
								android:padding="3dp"
								android:src="@drawable/content_cut"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview11"
								android:focusable="false"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Cut"
								android:textSize="11sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/detailsMode"
							android:layout_width="40dp"
							android:layout_height="match_parent"
							android:layout_margin="3dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview9"
								android:focusable="false"
								android:layout_width="30dp"
								android:layout_height="30dp"
								android:padding="3dp"
								android:src="@drawable/content_information_1"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview13"
								android:focusable="false"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Details"
								android:textSize="11sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/deleteMode"
							android:layout_width="40dp"
							android:layout_height="match_parent"
							android:layout_margin="3dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview4"
								android:focusable="false"
								android:layout_width="30dp"
								android:layout_height="30dp"
								android:padding="3dp"
								android:src="@drawable/content_delete"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview8"
								android:focusable="false"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Delete"
								android:textSize="11sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/selectMode"
							android:layout_width="40dp"
							android:layout_height="match_parent"
							android:layout_margin="3dp"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="vertical">
							<ImageView
								android:id="@+id/imageview1"
								android:focusable="false"
								android:layout_width="30dp"
								android:layout_height="30dp"
								android:padding="3dp"
								android:src="@drawable/content_checkstate1"
								android:scaleType="fitCenter" />
							<TextView
								android:id="@+id/textview6"
								android:focusable="false"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Select"
								android:textSize="11sp"
								android:textColor="#FFFFFF" />
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<FrameLayout
				android:id="@+id/framelayout1"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear1"
					android:layout_width="match_parent"
					android:layout_height="80dp"
					android:padding="4dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal"
					android:layout_weight="1"
					android:animateLayoutChanges="true">
					<LinearLayout
						android:id="@+id/linear9"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical">
						<FrameLayout
							android:id="@+id/linear10"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:orientation="horizontal">
							<ImageView
								android:id="@+id/picture"
								android:focusable="false"
								android:layout_width="70dp"
								android:layout_height="45dp"
								android:src="@drawable/default_image"
								android:scaleType="fitCenter" />
							<LinearLayout
								android:id="@+id/cb_container"
								android:layout_width="70dp"
								android:layout_height="45dp"
								android:gravity="center_horizontal|center_vertical"
								android:orientation="vertical">
								<CheckBox
									android:id="@+id/checkbox1"
									android:focusable="false"
									android:layout_width="35dp"
									android:layout_height="35dp"
									android:paddingTop="15dp"
									android:paddingBottom="15dp"
									android:gravity="center_horizontal|center_vertical"
									android:textSize="10sp"
									android:textColor="#000000"
									android:scaleX="2.0"
									android:scaleY="2.0"
									android:checked="true"
									android:layout_gravity="center_horizontal|center_vertical"
									android:button="@drawable/selector" />
							</LinearLayout>
						</FrameLayout>
						<TextView
							android:id="@+id/badge"
							android:focusable="false"
							android:layout_width="35dp"
							android:layout_height="wrap_content"
							android:gravity="center_horizontal|center_vertical"
							android:text=".e"
							android:textSize="12sp"
							android:textColor="#9E9E9E"
							android:singleLine="true"
							android:ellipsize="marquee"
							android:marqueeRepeatLimit="marquee_forever"
							android:scrollHorizontally="true" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear5"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:gravity="center_vertical"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear6"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:gravity="center_horizontal|center_vertical"
							android:orientation="horizontal"
							android:layout_weight="1">
							<LinearLayout
								android:id="@+id/linear11"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:gravity="center_vertical"
								android:orientation="vertical"
								android:layout_weight="1">
								<TextView
									android:id="@+id/textview1"
									android:focusable="false"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:paddingLeft="4dp"
									android:paddingTop="8dp"
									android:paddingRight="8dp"
									android:paddingBottom="8dp"
									android:gravity="left|center_vertical"
									android:text="File"
									android:textSize="16sp"
									android:textColor="#FFFFFF"
									android:layout_weight="1"
									android:ellipsize="marquee"
									android:marqueeRepeatLimit="marquee_forever"
									android:scrollHorizontally="true" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/tl"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:gravity="right|center_vertical"
								android:orientation="vertical">
								<TextView
									android:id="@+id/length"
									android:focusable="false"
									android:layout_width="match_parent"
									android:layout_height="wrap_content"
									android:paddingLeft="8dp"
									android:paddingRight="8dp"
									android:gravity="right|center_vertical"
									android:text="0KB"
									android:textSize="12sp"
									android:textColor="#FFFFFF"
									android:layout_weight="1"
									android:ellipsize="marquee"
									android:marqueeRepeatLimit="marquee_forever"
									android:scrollHorizontally="true" />
								<TextView
									android:id="@+id/date"
									android:focusable="false"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:paddingLeft="8dp"
									android:paddingRight="8dp"
									android:text="01.jan.2000"
									android:textSize="12sp"
									android:textColor="#FFFFFF"
									android:ellipsize="marquee"
									android:marqueeRepeatLimit="marquee_forever"
									android:scrollHorizontally="true" />
							</LinearLayout>
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</FrameLayout>
		</com.inflps.codexyz.SwipeRevealLayout>
	</LinearLayout>
</LinearLayout>
