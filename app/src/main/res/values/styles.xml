<?xml version="1.0" encoding="UTF-8"?><resources>
    <!--InfLps(ware)-->
    <!-- Popup Animation Styles -->
    <style name="PopupAnimation">
        <item name="android:windowEnterAnimation">@anim/popup_animation</item>
        <item name="android:windowExitAnimation">@anim/popup_hide_animation</item>
    </style>
    <style name="PopupAnimation_BottomRightScale">
        <item name="android:windowEnterAnimation">@anim/popup_scale_from_bottom_right</item>
        <item name="android:windowExitAnimation">@anim/popup_scale_to_bottom_right</item>
    </style>
    <!-- Base Application Theme -->
    <style name="AppTheme" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorControlHighlight">@color/colorControlHighlight</item>
        <item name="colorControlNormal">@color/colorControlNormal</item>
    </style>
    <!-- Translucent Dialog Theme-->
    <style name="CustomDialogTheme" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.5</item>
    </style>
    <!-- Translucent BottomSheet Theme-->
    <style name="BottomSheet.Transparent" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
</resources>
